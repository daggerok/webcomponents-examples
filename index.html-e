<!DOCTYPE html>
<html dir="ltr" lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=5.0">
  <title>Stencil Starter App</title>
  <script src="/webcomponents-examples/build/app.js"></script>

  <meta name="theme-color" content="#16161d">

  <meta name="apple-mobile-web-app-capable" content="yes">
  <link rel="apple-touch-icon" href="/webcomponents-examples/assets/img/icon.png">

  <link rel="manifest" href="/webcomponents-examples/manifest.json">
  <link rel="icon" type="image/x-icon" href="/webcomponents-examples/assets/icon/favicon.ico">

</head>
<body>

  <my-name first="Stencil" last="JS"></my-name>


  <script>
    if ('serviceWorker' in navigator && location.protocol !== 'file:') {
      // auto-unregister service worker during dev mode
      navigator.serviceWorker.ready.then(registration => {
        registration.unregister().then(() => {
          location.reload(true);
        });
      });
    }
  </script>

</body>
</html>
